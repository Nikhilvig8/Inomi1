                                </div>
                                <div class="table-responsive cmn-table">
                                    <table class="table table-striped" id="CollegeDetails">
                                        <thead>
                                            <tr>
                                                <th scope="col">College Name</th>
                                                <th scope="col">Country</th>
                                                <th scope="col">International Rank</th>
                                                <th scope="col">Indian Rank</th>
                                                <th scope="col">US News Ranking</th>
                                                <th scope="col">QS World Ranking</th>
                                                <th scope="col">Grade Profile</th>
                                                <th scope="col">Deadline Date</th>
                                                <th scope="col"></th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (ViewBag.CollegeDetails != null)
                                            {
                                                foreach (var dr in ViewBag.CollegeDetails.Rows)
                                                {
                                                    <tr>
                                                        <td>@dr["NameOfCollege"]</td>
                                                        <td>@dr["Country"]</td>
                                                        <td>@dr["IRank"]</td>
                                                        <td>@dr["IndRank"]</td>
                                                        <td>@dr["USNewsRanking"]</td>
                                                        <td>@dr["QSWorldRanking"]</td>
                                                        <td>@dr["GrandeProfile"]</td>

                                                        @if (dr["AppDate"] < 0)
                                                        {
                                                            <td style="color:white; font-weight: bold; background-color:#fd441f">@dr["ApplicationDeadline"]</td>
                                                        }
                                                        else if (dr["AppDate"] <= 5 && dr["AppDate"] >= 0)
                                                        {
                                                            <td style="color:white; font-weight: bold; background-color:#ff8067">@dr["ApplicationDeadline"]</td>
                                                        }
                                                        else
                                                        {
                                                            <td style="color:white; font-weight: bold; background-color:#6baa0f">@dr["ApplicationDeadline"]</td>
                                                        }


                                                        <td><label id="ID" style="display: none;">@dr["Id"]</label><button type="button" class="fas fa-trash" id="delete"></button><button type="button" class="fas fa-pencil-alt" id="edit"></button></td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>


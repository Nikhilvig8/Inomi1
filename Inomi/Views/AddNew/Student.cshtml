
@{
    ViewBag.Title = "Student";
}



<div id="page-content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12 content-width dashboard">
                <div class="row">
                    <h2 class="col-md-12">Add New</h2>
                </div>
            </div>
        </div>
        <div class="report-details-cover">
            <div>
                @if (TempData["Message"] != null || TempData["Message"].ToString() != "")
                {
                    <label style="font:bold;color:forestgreen;font-size: medium;margin-right: 575px;" id="msgErroStudent" class="diff-hd">
                        @TempData["Message"]
                    </label>

                }
                <label id="tab" style="display:none"> @TempData["tab"]</label>
            </div>
            <div class="row">
                <div class="col-md-12">

                    <nav class="nav-tabs-cover">
                        <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                            <button type="button" class="nav-item nav-link active" id="studenttab" data-toggle="tab" href="#student" role="tab" aria-controls="student" aria-selected="true">
                                <span>Student</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="counsellortab" data-toggle="tab" href="#counsellor" role="tab" aria-controls="counsellor" aria-selected="false">
                                <span>Counsellor</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="collegetab" data-toggle="tab" href="#college" role="tab" aria-controls="college" aria-selected="false">
                                <span>College</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="countrytab" data-toggle="tab" href="#country" role="tab" aria-controls="country" aria-selected="false">
                                <span>Country</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="producttab" data-toggle="tab" href="#product" role="tab" aria-controls="product" aria-selected="false">
                                <span>Product</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="coursetab" data-toggle="tab" href="#course" role="tab" aria-controls="course" aria-selected="false">
                                <span>Course</span>
                            </button>
                            <button style="display:none" type="button" class="nav-item nav-link" id="tasktab" data-toggle="tab" href="#task" role="tab" aria-controls="task" aria-selected="false">
                                <span>Task</span>
                            </button>
                            <button style="display:none" type="button" class="nav-item nav-link" id="testtab" data-toggle="tab" href="#test" role="tab" aria-controls="test" aria-selected="false">
                                <span>Test</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="formattab" data-toggle="tab" href="#format" role="tab" aria-controls="format" aria-selected="false">
                                <span>Format</span>
                            </button>
                        </div>
                    </nav>

                    <div class="tab-content px-sm-0 clearfix" id="nav-tabContent">


                        <div class="tab-pane" id="studentGrid" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="students-table-cover">
                                <div class="report-details">
                                    <h2 class="diff-hd">Student List</h2>
                                    <div class="reports-hdr">
                                        <div class="d-flex">
                                            <button type="submit" class="btn blue-btn add-new" id="AddNew">Add New</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive cmn-table">
                                    <table class="table table-striped" id="StudentDetails">
                                        <thead>
                                            <tr>
                                                <th scope="col">Name</th>
                                                <th scope="col">Grade</th>
                                                <th scope="col">School</th>
                                                <th scope="col">Phone</th>
                                                <th scope="col">Email</th>
                                                <th scope="col">Gender</th>
                                                <th scope="col">Product</th>
                                                <th scope="col">Status</th>
                                                <th scope="col"></th>
                                                <th scope="col"></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (ViewBag.StudentDetails != null)
                                            {
                                                foreach (var dr in ViewBag.StudentDetails.Rows)
                                                {
                                            <tr>
                                                <td>@dr["FirstName"]</td>
                                                <td>@dr["Grade"]</td>
                                                <td>@dr["School"]</td>
                                                <td>@dr["Phone"]</td>
                                                <td>@dr["Email"]</td>
                                                <td>@dr["Gender"]</td>
                                                <td>@dr["Product"]</td>
                                                <td>@dr["Status"]</td>
                                                <td><label id="ID" style="display: none;">@dr["ID"]</label><button type="button" class="fas fa-trash" id="delete"></button><button type="button" class="fas fa-pencil-alt" id="edit"></button></td>

                                                @if (dr["Status"] == "Accepted")
                                                {
                                                    <td>
                                                        <a href="javascript:void;" class="button13" id="Rejected">Reject</a>
                                                    </td>
                                                }
                                                else if (dr["Status"] == "Rejected")
                                                {
                                                    <td>
                                                        <a href="javascript:void;" class="button11" id="Approved">Accept</a>
                                                    </td>
                                                }
                                                else if (dr["Status"] == "N/A")
                                                {
                                                    <td>
                                                        <a href="javascript:void;" class="button11" id="Approved">Accept</a>
                                                    </td>
                                                }
                                            </tr>
                                                }
                                            }

                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>


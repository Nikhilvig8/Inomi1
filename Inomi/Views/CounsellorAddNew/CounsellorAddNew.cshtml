                                    <label style="display:none;color:red" id="msgTaskDeadline"></label>

                                </div>
                                <div class="d-flex bttn-box col-12 mb-0 mt-2">
                                    <button type="submit" id="TaskSubmit" class="btn blue-btn">Submit</button>
                                    <button type="reset" id="TaskBack" class="btn dark-grey-btn back-hide-show">Back</button>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="college" role="tabpanel" aria-labelledby="nav-contact-tab">
                            <div class="row">
                                <div class="form-group col-sm-3 col-12">
                                    <label>Name of University/College<sup>*</sup></label>
                                    <input type="text" id="CollegeName" class="form-control">
                                    <input style="display:none" type="text" id="CollegeId" class="form-control">
                                    <label style="display:none;color:red" id="msgCollegeName"></label>
                                </div>

                                <div class="form-group col-sm-3 col-12">
                                    <label>Campus<sup>*</sup></label>
                                    <input type="text" id="CampusName" class="form-control">
                                    <label style="display:none;color:red" id="msgCampusName"></label>
                                </div>

                                <div class="form-group col-sm-3 col-6">
                                    <label>Country<sup>*</sup></label>
                                    <div class="selectdiv">
                                        @Html.DropDownList("Name", (IEnumerable<SelectListItem>)ViewBag.CountryList, "--Select--", new { @class = "selectdiv", @id = "CollegeCountry" })
                                    </div>
                                    <label style="display:none;color:red" id="msgCollegeCountry"></label>
                                </div>

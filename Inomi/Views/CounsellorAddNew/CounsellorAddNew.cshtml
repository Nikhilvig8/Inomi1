
@{
    ViewBag.Title = "Student";
}



<div id="page-content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12 content-width dashboard">
                <div class="row">
                    <h2 class="col-md-12">Add New</h2>
                </div>
            </div>
        </div>
        <div class="report-details-cover">

            @if (TempData["Message"] != null || TempData["Message"].ToString() != "")
            {
                <div>
                    <label style="font:bold;color:forestgreen;font-size: medium;margin-right: 575px;" id="msgErroStudent" class="diff-hd">
                        @TempData["Message"]
                    </label>

                </div>
            }
            <label id="tab" style="display:none"> @TempData["tab"]</label>

            <div class="row">
                <div class="col-md-12">

                    <nav class="nav-tabs-cover">
                        <div class="nav nav-tabs nav-fill" id="nav-tab" role="tablist">
                            <button style="display:none" type="button" class="nav-item nav-link " id="tasktab" data-toggle="tab" href="#task" role="tab" aria-controls="task" aria-selected="true">
                                <span>Task</span>
                            </button>

                            <button type="button" class="nav-item nav-link active" id="collegetab" data-toggle="tab" href="#college" role="tab" aria-controls="college" aria-selected="false">
                                <span>College</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="countrytab" data-toggle="tab" href="#country" role="tab" aria-controls="country" aria-selected="false">
                                <span>Country</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="coursetab" data-toggle="tab" href="#course" role="tab" aria-controls="course" aria-selected="false">
                                <span>Course</span>
                            </button>
                            <button type="button" class="nav-item nav-link" id="projecttab" data-toggle="tab" href="#project" role="tab" aria-controls="course" aria-selected="false">
                                <span>Project</span>
                            </button>
                        </div>
                    </nav>

                    <div class="tab-content px-sm-0 clearfix" id="nav-tabContent">

                        <div style="display:none" class="tab-pane" id="TaskGrid" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="students-table-cover">
                                <div class="report-details">
                                    <h2 class="diff-hd">Task List</h2>
                                    <div class="reports-hdr">
                                        <div class="d-flex">
                                            <button type="submit" class="btn blue-btn add-new" id="TaskAddNew">Add New</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive cmn-table">
                                    <table class="table table-striped" id="TaskDetails">
                                        <thead>
                                            <tr>
                                                <th scope="col">Student</th>
                                                <th scope="col">Task</th>
                                                <th scope="col">Deadline</th>
                                                <th scope="col"></th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (ViewBag.TaskDetails != null)
                                            {
                                                foreach (var dr in ViewBag.TaskDetails.Rows)
                                                {
                                                    <tr>
                                                        <td>@dr["Counsellor"]</td>
                                                        <td>@dr["Task"]</td>
                                                        <td>@dr["Deadline"]</td>
                                                        <td><label id="ID" style="display: none;">@dr["Id"]</label><button type="button" class="fas fa-trash" id="delete"></button><button type="button" class="fas fa-pencil-alt" id="edit"></button></td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="CollegeGrid" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="students-table-cover">
                                <div class="report-details">
                                    <h2 class="diff-hd">College List</h2>
                                    <div class="d-flex justify-content-around">
                                        <div class="p2">
                                            <img src="/images/red-sq.jpg" height="15px" alt="icons">
                                            <span>DATE OVER.</span>
                                        </div>
                                        <div class="p2">
                                            <img src="/images/orange-sq.jpg" height="15px" alt="icons"><span> EARLY DECISION.</span>
                                        </div>
                                        <div class="p2">
                                            <img src="/images/green-square.jpg" height="15px" alt="icons">
                                            <span>UPCOMING DATE.</span>
                                        </div>

                                    </div>
                                    <div class="reports-hdr">
                                        <div class="d-flex">
                                            <button type="submit" class="btn blue-btn add-new" id="CollegeAddNew">Add New</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive cmn-table">
                                    <table class="table table-striped" id="CollegeDetails">
                                        <thead>
                                            <tr>
                                                <th scope="col">Name</th>
                                                <th scope="col">Country</th>
                                                <th scope="col">International Rank</th>
                                                <th scope="col">Indian Rank</th>
                                                <th scope="col">US News Ranking</th>
                                                <th scope="col">QS World Ranking</th>
                                                <th scope="col">Grade Profile</th>
                                                <th scope="col">Deadline Date</th>
                                                <th scope="col"></th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (ViewBag.CollegeDetails != null)
                                            {
                                                foreach (var dr in ViewBag.CollegeDetails.Rows)
                                                {
                                                    <tr>
                                                        <td>@dr["NameOfCollege"]</td>
                                                        <td>@dr["Country"]</td>
                                                        <td>@dr["IRank"]</td>
                                                        <td>@dr["IndRank"]</td>
                                                        <td>@dr["USNewsRanking"]</td>
                                                        <td>@dr["QSWorldRanking"]</td>
                                                        <td>@dr["GrandeProfile"]</td>

                                                        @if (dr["AppDate"] < 0)
                                                        {
                                                            <td style="color:white; font-weight: bold; background-color:#fd441f">@dr["ApplicationDeadline"]</td>
                                                        }
                                                        else if (dr["AppDate"] <= 5 && dr["AppDate"] >= 0)
                                                        {
                                                            <td style="color:white; font-weight: bold; background-color:#ff8067">@dr["ApplicationDeadline"]</td>
                                                        }
                                                        else
                                                        {
                                                            <td style="color:white; font-weight: bold; background-color:#6baa0f">@dr["ApplicationDeadline"]</td>
                                                        }


                                                        <td><label id="ID" style="display: none;">@dr["Id"]</label><button type="button" class="fas fa-trash" id="delete"></button><button type="button" class="fas fa-pencil-alt" id="edit"></button></td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="CountryGrid" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="students-table-cover">
                                <div class="report-details">

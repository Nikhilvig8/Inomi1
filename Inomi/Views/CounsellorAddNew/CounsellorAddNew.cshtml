                                                        <td>@dr["JobProspect"]</td>
                                                        <td>@dr["VisaProcess"]</td>
                                                        <td>@dr["Link"]</td>
                                                        <td><label id="ID" style="display: none;">@dr["Id"]</label><button type="button" class="fas fa-trash" id="delete"></button><button type="button" class="fas fa-pencil-alt" id="edit"></button></td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="CourseGrid" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="students-table-cover">
                                <div class="report-details">
                                    <h2 class="diff-hd">Course List</h2>
                                    <div class="reports-hdr">
                                        <div class="d-flex">
                                            <button type="submit" class="btn blue-btn add-new" id="CourseAddNew">Add New</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive cmn-table">
                                    <table class="table table-striped" id="CourseDetails">
                                        <thead>
                                            <tr>
                                                <th scope="col">Name of course</th>
                                                <th scope="col">College</th>
                                                <th scope="col">Career</th>
                                                <th scope="col">Link</th>
                                                <th scope="col"></th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (ViewBag.CourseDetails != null)
                                            {
                                                foreach (var dr in ViewBag.CourseDetails.Rows)
                                                {
                                                    <tr>
                                                        <td>@dr["NameOfCourse"]</td>
                                                        <td>@dr["College"]</td>
                                                        <td>@dr["Career"]</td>
                                                        <td>@dr["Link"]</td>
                                                        <td><label id="ID" style="display: none;">@dr["Id"]</label><button type="button" class="fas fa-trash" id="delete"></button><button type="button" class="fas fa-pencil-alt" id="edit"></button></td>
                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="ProjectGrid" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="students-table-cover">
                                <div class="report-details">
                                    <h2 class="diff-hd">Project List</h2>
                                    <div class="reports-hdr">
                                        <div class="d-flex">
                                            <button type="submit" class="btn blue-btn add-new" id="ProjectAddNew">Add New</button>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive cmn-table">
                                    <table class="table table-striped" id="ProjectDetails">
                                        <thead>
                                            <tr>
                                                <th scope="col">Student Name</th>
                                                <th scope="col">Project Name</th>
                                                <th scope="col">Description</th>
                                                <th scope="col">Milestone</th>
                                                <th scope="col">Deadline</th>
                                                <th scope="col">Status</th>
                                                <th scope="col">Details</th>
                                                <th scope="col">Action</th>
                                                <th scope="col"></th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (ViewBag.ProjectDetails != null)
                                            {
                                                foreach (var dr in ViewBag.ProjectDetails.Rows)
                                                {
                                                    <tr>
                                                        <td>@dr["Student"]</td>
                                                        <td>@dr["Name"]</td>
                                                        <td>@dr["Description"]</td>
                                                        <td>@dr["Milestone"]</td>
                                                        <td>@dr["deadline"]</td>
                                                        @if (@dr["status"] == "Done")
                                                        {
                                                            <td style="background-color:#95c84a; font-weight:bold;color:white">@dr["status"]</td>
                                                        }
                                                        else
                                                        {
                                                            <td style="background-color:#fc5c3c; font-weight:bold;color:white">@dr["status"]</td>
                                                        }
                                                        @if (@dr["status"] == "Done")
                                                        {
                                                            <td><label id="ID" style="display: none;">@dr["MID"]</label><button type="button" class="fas fa-check" id="ViewModel"></button></td>
                                                            <td></td>
                                                            <td></td>
                                                        }
                                                        else
                                                        {
                                                            <td><label id="ID" style="display: none;">@dr["MID"]</label><button type="button" class="fas fa-check" id="ViewModel"></button></td>
                                                            <td><label id="ID" style="display: none;">@dr["MID"]</label><button type="button" class="fas fa-check" id="Action"></button></td>
                                                            <td><label id="ID" style="display: none;">@dr["ProjectID"]</label><button type="button" class="fas fa-trash" id="delete"></button><button type="button" class="fas fa-pencil-alt" id="edit"></button></td>
                                                        }

                                                    </tr>
                                                }
                                            }
                                        </tbody>
                                    </table>

                                </div>
                            </div>
                        </div>


                        <div style="display:none" class="tab-pane" id="task" role="tabpanel" aria-labelledby="nav-about-tab">
                            <div class="row">
                                <div class="form-group col-sm-3 col-12">
                                    <label>Student<sup>*</sup></label>
                                    @*<input type="text" id="TaskCounsellor" class="form-control">*@
                                    <div class="selectdiv">
                                        @*  @Html.DropDownList("Name", (IEnumerable<SelectListItem>)ViewBag.CounsellorList, "--Select--", new { @class = "selectdiv", @id = "TaskCounsellor" })*@
                                        @Html.DropDownList("Name", (IEnumerable<SelectListItem>)ViewBag.StudentList, new { @class = "selectdiv", @id = "MddlStudent", @title = "Search by student" })

                                    </div>
                                    <input style="display:none" type="text" id="TaskId" class="form-control">
                                    <label style="display:none;color:red" id="msgTaskCounsellor"></label>

                                </div>

@model Inomi.Models.Student.Dashboard

<!-- Modal Header -->
<div class="modal-header">
    <h4 class="modal-title">Add Test Report</h4>
    <button type="button" class="close" data-dismiss="modal">
        <span>x</span>
    </button>
</div>

<!-- Modal body -->
<div class="modal-body">
    @Html.Hidden("id", @Model.EditTestReport.id)
    <div class="row">

        <div class="form-group col-sm-6 col-12">
            <label>Test Name</label>
            @Html.EditorFor(m=>m.EditTestReport.TestName, new { htmlAttributes = new { @class = "form-control", @id = "TestName", @Name = "TestName" } })
        </div>
        <div class="form-group col-sm-6 col-12">
            <label>Score</label>
            @Html.EditorFor(m=>m.EditTestReport.Score, new { htmlAttributes = new { @class = "form-control", @id = "Score", @Name = "Score" } })
        </div>
    </div>
</div>
<!-- Modal footer -->
<div class="modal-footer">
    <button type="button" class="btn blue-btn" id="UpdateTestreportbutton">Update</button>
</div>